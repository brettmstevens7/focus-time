{
	"name": "focus-time",
	"displayName": "Focus Time",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
            "type": "object",
            "title": "Pomodoro",
            "properties": {
                "intervalLength": {
                    "type": "number",
                    "default": 25,
                    "description": "Interval length (minutes)"
				},
				"pauseLength": {
                    "type": "number",
                    "default": 5,
                    "description": "Interval length (minutes)"
				},
				"cycles": {
                    "type": "number",
                    "default": 3,
                    "description": "Interval length (minutes)"
				}
            }
        },
		"commands": [
			{
				"command": "extension.startPomodoro",
				"title": "Start Pomodoro"
			},
			{
				"command": "extension.pausePomodoro",
				"title": "Pause Pomodoro"
			},
			{
				"command": "extension.resetPomodoro",
				"title": "Reset Pomodoro"
			},
			{
				"command": "extension.Pomodoro",
				"title": "Pomodoro"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
