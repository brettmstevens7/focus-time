{
	"name": "focus-time",
	"displayName": "Focus Time",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Pomodoro",
			"properties": {
				"intervalLength": {
					"type": "number",
					"default": 25,
					"description": "Interval length (minutes)"
				},
				"pauseLength": {
					"type": "number",
					"default": 5,
					"description": "Interval length (minutes)"
				},
				"cycles": {
					"type": "number",
					"default": 3,
					"description": "Interval length (minutes)"
				}
			}
		},
		"commands": [
			{
				"command": "extension.startPomodoroCmd",
				"title": "Start Pomodoro"
			},
			{
				"command": "extension.pausePomodoroCmd",
				"title": "Pause Pomodoro"
			},
			{
				"command": "extension.resetPomodoroCmd",
				"title": "Reset Pomodoro"
			},
			{
				"command": "extension.pomodoroCmd",
				"title": "Pomodoro"
			},
			{
				"command": "extension.getPomodoroMetricsCmd",
				"title": "Get Pomodoro Metrics"
			},
			{
				"command": "extension.toggleDoNotDisturbCmd",
				"title": "Toggle do not disturb"
			},
			{
				"command": "extension.toggleDarkModeCmd",
				"title": "Toggle dark mode"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "extension.getPomodoroMetricsCmd"
				},
				{
					"command": "extension.toggleDoNotDisturbCmd"
				},
				{
					"command": "extension.toggleDarkModeCmd"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"@sindresorhus/do-not-disturb": "^1.0.0"
	}
}
